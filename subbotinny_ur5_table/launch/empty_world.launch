<launch>
    <arg name="model" default="$(find subbotinny_ur5_table)/urdf/table.urdf.xacro" />
    <arg name="world_name" default="worlds/empty.world" />
    <param name="robot_description" command="xacro '$(arg model)' " />

    <!-- Launch Gazebo -->
    <include file="$(find gazebo_ros)/launch/empty_world.launch">
        <arg name="world_name" value="$(arg world_name)" />
    </include>
    <node name="spawn_model" pkg="gazebo_ros" type="spawn_model" args="-urdf -model ur5_table -param robot_description -z 0.1" respawn="false" />

</launch>